file = {:C$\backslash$:/Users/chold/Dropbox/Documents/Academic/Academic.Current/UCR/Research/Papers/Tutwiler et al. - 2016 - Kinetics and mechanics of clot contraction are governed by the molecular and cellular composition of the blood.pdf:pdf},
isbn = {2015056475},
issn = {15280020},
journal = {Blood},
keywords = {A new dynamic quantitative clot contraction assay,And various blood components,Clot contraction has 3 phases differentially affec,RBC compaction},
month = {jan},
