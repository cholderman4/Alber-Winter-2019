file = {:C$\backslash$:/Users/chold/Dropbox/Documents/Academic/Academic.Current/UCR/Research/Papers/Loessner et al. - 2013 - A multiscale road map of cancer spheroids - incorporating experimental and mathematical modelling to understand.pdf:pdf},
isbn = {1477-9137 (Electronic)$\backslash$r0021-9533 (Linking)},
issn = {0021-9533},
journal = {Journal of Cell Science},
