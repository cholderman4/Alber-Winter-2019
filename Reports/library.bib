isbn = {1553-7358},
issn = {1553734X},
journal = {PLoS Computational Biology},
number = {2},
pmid = {23468612},
title = {{Dynamic Modeling of Cell Migration and Spreading Behaviors on Fibronectin Coated Planar Substrates and Micropatterned Geometries}},
