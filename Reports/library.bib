isbn = {1091-6490 (Electronic) 0027-8424 (Linking)},
issn = {0027-8424},
journal = {Proceedings of the National Academy of Sciences},
month = {feb},
number = {9},
pages = {E1617--E1626},
pmid = {28196892},
title = {{Modeling the two-way feedback between contractility and matrix realignment reveals a nonlinear mode of cancer cell invasion}},
